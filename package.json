{
  "name": "nisper",
  "version": "0.3.3",
  "description": "A RPC lib based on websocket protocol and nisp language",
  "main": "lib/index.js",
  "scripts": {
    "test": "no test",
    "prepublish": "no lint"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "nisp": "0.6.2",
    "ws": "1.1.1",
    "yaku": "0.17.3"
  },
  "files": [
    "lib"
  ],
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6
    },
    "rules": {
      "indent": [
        2,
        4
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ],
      "no-trailing-spaces": 2,
      "space-before-function-paren": [
        2,
        "always"
      ],
      "no-unused-vars": 0,
      "eqeqeq": [
        2,
        "allow-null"
      ]
    },
    "env": {
      "browser": true,
      "node": true
    },
    "extends": "eslint:recommended"
  },
  "devDependencies": {
    "eslint": "3.12.0",
    "junit": "1.4.6",
    "msgpack-js": "0.3.0",
    "nokit": "0.23.9"
  }
}
